    "chai": "^4.3.7",
    "commander": "^10.0.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "ethers": "5.7.2",
    "hardhat": "^2.14.0",
    "hardhat-tracer": "^2.3.2",
    "lodash": "^4.17.21",
    "path": "^0.12.7",
    "prettier": "^2.8.8",
